# This is the order in which things must be built to work, and also the names
# of the directories things will be cloned into from REMOTES below.
BUILDORDER=( \
	cinnamon-desktop \
	muffin \
	cjs \
	cinnamon-settings-daemon \
	cinnamon-session \
	cinnamon-translations \
	cinnamon-menus \
	cinnamon-control-center \
	cinnamon \
	cinnamon-screensaver \
	cinnamon-bluetooth \
	)

# These are the remotes which will be set as upstream. Must be in the same order
# as the names in build order.
REMOTES=( \
	"https://github.com/linuxmint/cinnamon-desktop.git" \
	"https://github.com/linuxmint/muffin.git" \
	"https://github.com/linuxmint/cjs.git" \
	"https://github.com/linuxmint//cinnamon-settings-daemon.git" \
	"https://github.com/linuxmint/cinnamon-settings-daemon.git" \
	"https://github.com/linuxmint/cinnamon-session.git" \
	"https://github.com/linuxmint/cinnamon-translations.git" \
	"https://github.com/linuxmint/cinnamon-menus.git" \
	"https://github.com/linuxmint/cinnamon-control-center.git" \
	"https://github.com/linuxmint/Cinnamon.git" \
	"https://github.com/linuxmint/cinnamon-screensaver.git" \
	"https://github.com/linuxmint/cinnamon-bluetooth.git" \
)
